{
  "name": "@janeirodigital/sai-server",
  "version": "1.0.0-rc.21",
  "description": "",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@janeirodigital/sai-server",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@janeirodigital/sai-server/^0.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@janeirodigital/sai-server/^0.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@janeirodigital/sai-server/^0.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@janeirodigital/sai-server/^0.0.0/dist/": "dist/"
  },
  "bin": {
    "sai-server": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "node dist/componentsMain.js",
    "redis:create": "docker run --name sai-redis -p 6379:6379 -d redis",
    "redis:delete": "docker rm -f sai-redis",
    "dev": "pnpm run redis:delete && pnpm run redis:create && pnpm run debug",
    "debug": "pnpm run build && node --inspect dist/componentsMain.js",
    "build": "pnpm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s dist -c dist/components -r ssv -i .componentsignore",
    "build:ts": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/",
    "test": "jest test/unit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write {src,test}/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@digita-ai/handlersjs-core": "^0.19.3",
    "@digita-ai/handlersjs-http": "^0.19.3",
    "@digita-ai/handlersjs-logging": "^0.19.3",
    "@inrupt/solid-client-authn-node": "^1.17.3",
    "@janeirodigital/interop-authorization-agent": "^1.0.0-rc.21",
    "@janeirodigital/interop-data-model": "^1.0.0-rc.21",
    "@janeirodigital/interop-utils": "^1.0.0-rc.21",
    "@janeirodigital/sai-api-messages": "1.0.0-rc.21",
    "@janeirodigital/sai-server-interfaces": "^1.0.0-rc.21",
    "@solid/access-token-verifier": "^2.0.5",
    "bullmq": "^4.12.4",
    "componentsjs": "^5.4.2",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "n3": "^1.17.1",
    "rxjs": "^7.8.1",
    "@solid-notifications/discovery": "^0.1.0",
    "@solid-notifications/subscription": "^0.1.0",
    "web-push": "^3.6.6",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@inrupt/solid-client-authn-core": "^1.17.3",
    "@rdfjs/types": "^1.1.0",
    "@types/body-parser": "^1.19.3",
    "@types/n3": "^1.16.2",
    "@types/web-push": "^3.6.1",
    "@types/yargs": "^17.0.28",
    "@solid-notifications/types": "^0.1.0"
  },
  "volta": {
    "node": "20.7.0"
  }
}
